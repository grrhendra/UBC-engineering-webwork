Context("Numeric");

#given
$e = random(15,25,.1);
$es = random(65,75,.1);
$ee = random(2,6,.1);

#computation 
$f = ($e-$ee)/($es-$ee)*100;

#set tolerance
Context()->flags->set(
tolerance=>.01,
);

