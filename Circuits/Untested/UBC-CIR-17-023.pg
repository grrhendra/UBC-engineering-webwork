	#"PGcourse.pl", 	 # Customization file for the course
   	);

##############################################################
#
#  Setup
#
#

Context("Numeric");
#given
$l = random(2,6,.1); #mA


#image scale
$imgScale =0.4;

#image aspect ratio
$width = $imgScale*583;
$height = $imgScale*574;

#computation
$omegaR = 1/(sqrt($l*.1));

#set tolerance
Context()->flags->set(
tolerance=>.01);

##############################################################
#
#  PGML 
#
#

BEGIN_PGML

	[@ image( "UBC-CIR-17-023.png", width=>[$width], height=>[$height]) @]*

	Calculate the following network's natural frequency, if [`C=0.1`] [`F`], [`R=28`] [`\Omega`], and [`L=[$l]`] [`H`].

	[`w_R=`] [_____]{"$omegaR"} [`rad/sec`]  

END_PGML

