DOCUMENT();

loadMacros(   
	"PGstandard.pl",     # Standard macros for PG language
	"MathObjects.pl",
	"PGML.pl",
	"parserPopUp.pl",
	"parserMultiAnswer.pl",
	"parserRadioButtons.pl",
	#"source.pl",        # allows code to be displayed on certain sites.
	#"PGcourse.pl", 	 # Customization file for the course
   	);

##############################################################
#
#  Setup
#
#

Context("Complex");

#given
$cap = random(.002,.008,.001); #mA


#image scale
$imgScale =0.4;

#image aspect ratio
$width = $imgScale*1292;
$height = $imgScale*636;

#computation
#48*C s^2 + 2 s + 10

$s1 = -2 + sqrt(Complex(4 - 4 * 48 * $cap * 10 + 0*i))/(2 * 48 * $cap);
$s2 = -2 - sqrt(Complex(4 - 4 * 48 * $cap * 10 + 0*i))/(2 * 48 * $cap);

$asdf = List("$s1","$s2")->cmp;

#set tolerance
Context()->flags->set(
tolerance=>.01);

##############################################################
#
#  PGML 
#
#

BEGIN_PGML

	[@ image( "UBC-CIR-17-024.png", width=>[$width], height=>[$height]) @]*

	Find the network's characteristic equation when the switch is flipped, [`C=[$cap]`] [`F`], [`R=48`] [`\Omega`] and [`L=4.8`] [`H`]. 

	[`Roots=`][_____]{$asdf}

END_PGML

