Context("Numeric");
#given
$Vj= random(1,5,1);# [m/s]

#computation
$Ans= 2*998* 0.1* 60*2*pi/60*0.2 *($Vj-0.2*2*pi)**2; #[W]

#set tolerance
Context()->flags->set(
tolerance=>.05,);

